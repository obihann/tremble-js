td.col-xs-2
  span.log-time= val.time
td.col-xs-1
  span.log-commit
    a(target="_blank", href="https://github.com/"+val.repo+"/commit/"+val.results[0].leftCommit)
      = val.results[0].cLeftCommit
td.col-xs-2
  span.log-commit
    a(target="_blank", href="https://github.com/"+val.repo+"/commit/"+val.results[0].rightCommit)
      = val.results[0].cRightCommit
td.col-xs-4
  table.table-center.table.table-hover.table-bordered
    thead
      tr.row
        td.col-xs-4
          h5 file
        td.col-xs-2
          h5 status
    tbody
      each res in val.results
        if res.status == true
          tr.row.success
            td.col-xs-8
              span.log-left= res.cLeft
            td.col-xs-2
              span.log-left pass
        else
          tr.row.danger
            td.col-xs-8
              span.log-left= res.cLeft
            td.col-xs-2
              span.log-left fail
